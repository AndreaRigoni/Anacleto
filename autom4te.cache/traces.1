@%:@ //// DOCKER SHELL  ///////////////////////////////////////////////////////// @%:@
@%:@ //////////////////////////////////////////////////////////////////////////// @%:@

DOCKER_CONTAINER=@S|@{DOCKER_CONTAINER}
DOCKER_IMAGE=@S|@{DOCKER_IMAGE}

abs_srcdir=@S|@{abs_srcdir}
abs_builddir=@S|@{abs_builddir}
user_id=@S|@{user_id}
user_group=@S|@{user_group}
user_groups=@S|@{user_groups}
user_home=@S|@{user_home}

>&2 echo "Docker: Entering container \@S|@{DOCKER_CONTAINER} ";
quoted_args="\@S|@(printf " %q" "\@S|@\@")"
if @<:@ -n "\@S|@{MAKESHELL}" @:>@; then
 \@S|@{MAKESHELL} \@S|@{quoted_args};
else
 @<:@ -t AS_ORIGINAL_STDIN_FD -o -t 0 @:>@ && INT=-ti || INT=
 docker exec \@S|@{INT} --user \@S|@{USER} \@S|@{DOCKER_CONTAINER} /bin/bash -l -c "cd \@S|@(pwd); export MAKESHELL=/bin/bash; export MAKEFLAGS=\@S|@{MAKEFLAGS}; export MFLAGS=\@S|@{MFLAGS}; /bin/bash \@S|@{quoted_args}";
fi
